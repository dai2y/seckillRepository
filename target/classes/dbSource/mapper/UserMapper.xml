<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="userDao">
    <insert id="insertUser" parameterType="dao.Entity.User">
        INSERT  INTO account(userName,userStatus,userOrg,createTime,createUser) VALUES (#{userName},#{userStatus},#{userOrg},#{createTime},#{createUser})
    </insert>
</mapper>